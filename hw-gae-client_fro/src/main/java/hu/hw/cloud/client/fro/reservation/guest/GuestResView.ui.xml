				</m:MaterialCollapsibleHeader>
				<m:MaterialCollapsibleBody addStyleNames="{style.padding0}">
					<m:MaterialRow>
						<m:MaterialColumn grid="s12 xl6" paddingLeft="0"
							paddingRight="0">
							<m:MaterialRow marginBottom="0">
								<m:MaterialTextBox ui:field="guestName"
									label="Vendégnév" value="Smith John, Mr." grid="s12"
									iconType="MORE_HORIZ" />

								<m:MaterialDatePicker ui:field="arrival"
									placeholder="Érkezés" grid="s4" format="yyyy.mm.dd" />
								<m:MaterialIntegerBox ui:field="nights"
									label="Éj" placeholder="Éj" grid="s4" />
								<m:MaterialDatePicker ui:field="departure"
									placeholder="Utazás" grid="s4" format="yyyy.mm.dd" />

								<m:MaterialDatePicker ui:field="birthsday"
									placeholder="Születés dátuma" grid="s4" format="yyyy.mm.dd" />
								<combobox:MaterialComboBox
									label="Korcsoport" grid="s4">
									<m:html.Option value="DBLB" text="Felnőtt" />
									<m:html.Option value="TWIN" text="Tini" />
									<m:html.Option value="SNGL" text="Gyerek" />
									<m:html.Option value="SNGL" text="Bébi" />
								</combobox:MaterialComboBox>
								<combobox:MaterialComboBox
									label="IFA mentesség" grid="s4">
									<m:html.Option value="DBLB" text="Fizet IFA-t" />
									<m:html.Option value="TWIN" text="18 év alatti" />
									<m:html.Option value="SNGL" text="Fekvőbeteg" />
									<m:html.Option value="SNGL" text="Tanuló" />
									<m:html.Option value="SNGL" text="Tulajdonos" />
									<m:html.Option value="SNGL" text="Egyházi" />
								</combobox:MaterialComboBox>

								<m:MaterialTextBox label="Útiokmámy" grid="s4" />
								<m:MaterialTextBox label="Rendszám" grid="s4" />
								<combobox:MaterialComboBox
									label="Határátlépés" grid="s4">
									<m:html.Option value="DBLB" text="Budapest" />
									<m:html.Option value="TWIN" text="Röszke" />
									<m:html.Option value="SNGL" text="Tompa" />
									<m:html.Option value="SNGL" text="Hegyeshalom" />
								</combobox:MaterialComboBox>
							</m:MaterialRow>
						</m:MaterialColumn>

