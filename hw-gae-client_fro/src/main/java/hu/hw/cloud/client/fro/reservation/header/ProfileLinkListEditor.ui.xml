<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">

	<ui:with field="i18nCore"
		type="hu.hw.cloud.client.core.i18n.CoreMessages" />

	<g:HTMLPanel>
		<g:FlowPanel ui:field="listPanel" />
		
		<m:MaterialButton ui:field="addButton"
			text="{i18nCore.roomEditorAvailabilityAdd}" waves="DEFAULT"
			backgroundColor="WHITE" textColor="BLACK" />

		<m:MaterialButton ui:field="deleteButton"
			text="{i18nCore.roomEditorAvailabilityRemove}" waves="DEFAULT"
			backgroundColor="WHITE" textColor="BLACK" />
	</g:HTMLPanel>
</ui:UiBinder> 