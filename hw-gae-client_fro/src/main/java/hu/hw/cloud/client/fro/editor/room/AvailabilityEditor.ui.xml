<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">

	<ui:with field="i18nCore"
		type="hu.hw.cloud.client.core.i18n.CoreMessages" />

	<g:HTMLPanel>
		<m:MaterialSwitch ui:field="available"
			enabled="false" grid="s6" />
		<m:MaterialDatePicker ui:field="date"
	placeholder="{i18nCore.roomEditorAvailabilityFromDate}" grid="s6" />
	</g:HTMLPanel>
</ui:UiBinder> 